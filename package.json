{
  "private": true,
  "name": "product-feedback-app",
  "description": "This is a solution to the Product feedback app challenge on Frontend Mentor. Frontend Mentor challenges help you improve your coding skills by building realistic projects.",
  "license": "MIT",
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --env production",
    "build:remix": "remix build",
    "db:seed": "prisma db seed",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "remix dev",
    "generate:css": "postcss styles --base styles --ext css --dir app/styles",
    "lint:code": "eslint --cache --ignore-path .gitignore \"**/*.[tj]s?(x)\"",
    "lint:css": "stylelint \"**/*.pcss\" --cache",
    "lint:fix": "eslint --cache --fix --ignore-path .gitignore \"**/*.[tj]s?(x)\"",
    "lint:fix-css": "stylelint \"**/*.pcss\" --cache --fix",
    "prepare": "husky install",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 'cypress open'",
    "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p ./cypress/tsconfig.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardcyrus/fm-rmx-product-feedback-app.git"
  },
  "keywords": [],
  "author": "Richard Cyrus <richard.cyrus@rcyrus.com> (https://www.richardcyrus.com)",
  "bugs": {
    "url": "https://github.com/richardcyrus/fm-rmx-product-feedback-app/issues"
  },
  "homepage": "https://github.com/richardcyrus/fm-rmx-product-feedback-app#readme",
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "@reach/listbox": "^0.16.2",
    "@reach/tabs": "^0.16.4",
    "@remix-run/node": "^1.4.3",
    "@remix-run/react": "^1.4.3",
    "@remix-run/vercel": "^1.4.3",
    "@vercel/node": "^1.14.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.14.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.4.3",
    "@remix-run/eslint-config": "^1.4.3",
    "@remix-run/serve": "^1.4.3",
    "@svgr/cli": "^6.2.1",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^12.1.5",
    "@testing-library/user-event": "^14.1.1",
    "@types/eslint": "^8.4.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@vitejs/plugin-react": "^1.3.1",
    "autoprefixer": "^10.4.5",
    "axe-core": "^4.4.1",
    "cssnano": "^5.1.7",
    "cypress": "^9.6.0",
    "cypress-axe": "^0.14.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "happy-dom": "^2.55.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.1",
    "micromatch": "^4.0.5",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.12",
    "postcss-cli": "^9.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-sort-media-queries": "^4.2.1",
    "prettier": "^2.6.2",
    "prisma": "^3.13.0",
    "start-server-and-test": "^1.14.0",
    "stylelint": "^14.8.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.4",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.9.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
