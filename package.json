{
  "private": true,
  "type": "module",
  "name": "product-feedback-app",
  "description": "This is a solution to the Product feedback app challenge on Frontend Mentor. Frontend Mentor challenges help you improve your coding skills by building realistic projects.",
  "license": "MIT",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "build": "remix build",
    "db:seed": "prisma db seed",
    "db:seed:dev": "dotenv -e .env.development -- npm run db:seed",
    "db:seed:test": "dotenv -e .env.test -- npm run db:seed",
    "db:setup": "npx prisma db push --force-reset",
    "db:setup:dev": "dotenv -e .env.development -- npm run db:setup",
    "db:setup:test": "dotenv -e .env.test -- npm run db:setup",
    "dev": "dotenv -e .env.development -- remix dev --manual",
    "fix": "concurrently npm:fix:*",
    "fix:code": "eslint --cache --cache-location ./node_modules/.cache/eslint --fix --ignore-path .gitignore \"**/*.[tj]s?(x)\"",
    "fix:css": "stylelint \"**/*.pcss\" --cache --cache-location ./node_modules/.cache/stylelint --fix",
    "format": "prettier --ignore-path .gitignore --write .",
    "lint": "concurrently --raw npm:lint:*",
    "lint:code": "eslint --cache --cache-location ./node_modules/.cache/eslint --ignore-path .gitignore \"**/*.[tj]s?(x)\"",
    "lint:css": "stylelint \"**/*.css\" --cache --cache-location ./node_modules/.cache/stylelint",
    "prepare": "husky install",
    "postinstall": "prisma generate",
    "start": "remix-serve ./build/index.js",
    "test": "npm run db:setup:test && npm run db:seed:test && npm run test:unit",
    "test:unit": "vitest run",
    "pretest:e2e": "npm run db:setup:test && npm run db:seed:test",
    "test:e2e": "dotenv -e .env.test -- playwright test --workers=1",
    "typecheck": "tsc -p tsconfig.json --noEmit && tsc -p ./cypress/tsconfig.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardcyrus/fm-rmx-product-feedback-app.git"
  },
  "keywords": [],
  "author": "Richard Cyrus <richard.cyrus@rcyrus.com> (https://www.richardcyrus.com)",
  "bugs": {
    "url": "https://github.com/richardcyrus/fm-rmx-product-feedback-app/issues"
  },
  "homepage": "https://github.com/richardcyrus/fm-rmx-product-feedback-app#readme",
  "dependencies": {
    "@prisma/client": "^5.8.0",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@remix-run/css-bundle": "2.5.1",
    "@remix-run/node": "2.5.1",
    "@remix-run/react": "2.5.1",
    "@remix-run/serve": "2.5.1",
    "@vercel/analytics": "^1.1.2",
    "@vercel/remix": "^2.5.1",
    "isbot": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.8.3",
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "^1.41.1",
    "@remix-run/dev": "2.5.1",
    "@svgr/cli": "^8.1.0",
    "@svgr/plugin-jsx": "^8.1.0",
    "@svgr/plugin-prettier": "^8.1.0",
    "@svgr/plugin-svgo": "^8.1.0",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/eslint": "^8.56.2",
    "@types/node": "^20.11.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "axe-core": "^4.8.3",
    "concurrently": "^8.2.2",
    "cssnano": "^6.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^13.0.6",
    "husky": "^8.0.3",
    "lint-staged": "^15.2.0",
    "micromatch": "^4.0.5",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-nested": "^6.0.1",
    "postcss-sort-media-queries": "^5.2.0",
    "prettier": "^3.2.2",
    "prisma": "^5.8.0",
    "stylelint": "^16.1.0",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-order": "^6.0.4",
    "tsx": "^4.7.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.3",
    "vitest": "^1.2.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 edge version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "sideEffects": false,
  "volta": {
    "node": "24.12.0"
  }
}
