{
  "private": true,
  "name": "product-feedback-app",
  "description": "This is a solution to the Product feedback app challenge on Frontend Mentor. Frontend Mentor challenges help you improve your coding skills by building realistic projects.",
  "license": "MIT",
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:css": "sass styles/:app/styles --style=compressed",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "run-p dev:*",
    "dev:css": "sass styles/:app/styles --color --no-source-map --watch",
    "dev:remix": "cross-env NODE_ENV=development PORT=7369 remix dev",
    "postinstall": "remix setup node"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardcyrus/fm-rmx-product-feedback-app.git"
  },
  "keywords": [],
  "author": "Richard Cyrus <richard.cyrus@rcyrus.com> (https://www.richardcyrus.com)",
  "bugs": {
    "url": "https://github.com/richardcyrus/fm-rmx-product-feedback-app/issues"
  },
  "homepage": "https://github.com/richardcyrus/fm-rmx-product-feedback-app#readme",
  "dependencies": {
    "@prisma/client": "^3.9.2",
    "@reach/listbox": "^0.16.2",
    "@reach/tabs": "^0.16.4",
    "@remix-run/react": "^1.2.2",
    "@remix-run/vercel": "^1.2.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.2",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.2",
    "@remix-run/eslint-config": "^1.2.2",
    "@remix-run/serve": "^1.2.2",
    "@svgr/cli": "^6.2.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "axe-core": "^4.4.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.1",
    "cypress-axe": "^0.14.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "prisma": "^3.9.2",
    "sass": "^1.49.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
