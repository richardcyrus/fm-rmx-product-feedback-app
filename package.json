{
  "private": true,
  "name": "product-feedback-app",
  "description": "This is a solution to the Product feedback app challenge on Frontend Mentor. Frontend Mentor challenges help you improve your coding skills by building realistic projects.",
  "license": "MIT",
  "prisma": {
    "seed": "node --require esbuild-register prisma/seed.ts"
  },
  "scripts": {
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --env production",
    "build:remix": "cross-env NODE_ENV=production remix build",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development PORT=7369 remix dev",
    "generate:css": "postcss styles --base styles --ext css --dir app/styles",
    "lint:code": "eslint --cache --ignore-path .gitignore \"**/*.[tj]s?(x)\"",
    "lint:css": "stylelint \"**/*.pcss\" --cache",
    "lint:fix-css": "stylelint \"**/*.pcss\" --cache --fix",
    "lint:fix": "eslint --cache --fix --ignore-path .gitignore \"**/*.[tj]s?(x)\"",
    "postinstall": "remix setup node",
    "prepare": "husky install",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardcyrus/fm-rmx-product-feedback-app.git"
  },
  "keywords": [],
  "author": "Richard Cyrus <richard.cyrus@rcyrus.com> (https://www.richardcyrus.com)",
  "bugs": {
    "url": "https://github.com/richardcyrus/fm-rmx-product-feedback-app/issues"
  },
  "homepage": "https://github.com/richardcyrus/fm-rmx-product-feedback-app#readme",
  "dependencies": {
    "@prisma/client": "^3.10.0",
    "@reach/listbox": "^0.16.2",
    "@reach/tabs": "^0.16.4",
    "@remix-run/react": "^1.2.3",
    "@remix-run/vercel": "^1.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.2.3",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.2.3",
    "@remix-run/eslint-config": "^1.2.3",
    "@remix-run/serve": "^1.2.3",
    "@svgr/cli": "^6.2.1",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "autoprefixer": "^10.4.2",
    "axe-core": "^4.4.1",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.1",
    "cypress": "^9.5.1",
    "cypress-axe": "^0.14.0",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.5",
    "micromatch": "^4.0.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.8",
    "postcss-cli": "^9.1.0",
    "postcss-nested": "^5.0.6",
    "postcss-sort-media-queries": "^4.2.1",
    "prettier": "^2.5.1",
    "prisma": "^3.10.0",
    "stylelint": "^14.5.3",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^25.0.0",
    "stylelint-order": "^5.0.0",
    "typescript": "^4.6.2"
  },
  "browserslist": [
    "defaults",
    "not IE 11",
    "maintained node versions"
  ],
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
