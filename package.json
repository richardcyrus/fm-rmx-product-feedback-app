{
  "private": true,
  "type": "module",
  "name": "product-feedback-app",
  "description": "This is a solution to the Product feedback app challenge on Frontend Mentor. Frontend Mentor challenges help you improve your coding skills by building realistic projects.",
  "license": "MIT",
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "scripts": {
    "build": "react-router build",
    "db:seed": "prisma db seed",
    "db:seed:dev": "dotenv -e .env.development -- pnpm run --silent db:seed",
    "db:seed:test": "dotenv -e .env.test -- pnpm --silent run db:seed",
    "db:setup": "npx prisma db push --force-reset",
    "db:setup:dev": "dotenv -e .env.development -- pnpm run --silent db:setup",
    "db:setup:test": "dotenv -e .env.test -- pnpm run --silent db:setup",
    "dev": "dotenv -e .env.development -- react-router dev",
    "fix": "concurrently pnpm:fix:*",
    "fix:code": "eslint  --fix \"**/*.[tj]s?(x)\"",
    "fix:css": "stylelint \"**/*.css\" --cache --cache-location ./node_modules/.cache/stylelint --fix",
    "format": "prettier --write .",
    "lint": "concurrently --raw pnpm:lint:*",
    "lint:code": "eslint \"**/*.[tj]s?(x)\"",
    "lint:css": "stylelint \"**/*.css\" --cache --cache-location ./node_modules/.cache/stylelint",
    "prepare": "husky",
    "postinstall": "prisma generate",
    "start": "react-router-serve build/server/index.js",
    "test": "pnpm run --silent db:setup:test && pnpm run --silent db:seed:test && pnpm run --silent test:unit",
    "test:unit": "vitest run",
    "pretest:e2e": "pnpm run --silent db:setup:test && pnpm run --silent db:seed:test",
    "test:e2e": "dotenv -e .env.test -- playwright test --workers=1",
    "typecheck": "react-router typegen && tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/richardcyrus/fm-rmx-product-feedback-app.git"
  },
  "keywords": [],
  "author": "Richard Cyrus <richard.cyrus@rcyrus.com> (https://www.richardcyrus.com)",
  "bugs": {
    "url": "https://github.com/richardcyrus/fm-rmx-product-feedback-app/issues"
  },
